# MiniJudge Environment Variables
# Rename this file to .env before using

# OpenAI API Key (for embeddings and LLM)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Path to Tesseract executable
# Example for Windows: C:\\Program Files\\Tesseract-OCR\\tesseract.exe
# Example for Mac (Homebrew): /usr/local/bin/tesseract
# Leave blank if Tesseract is in your PATH
TESSERACT_PATH=

# Default language for OCR (Default: eng)
OCR_LANGUAGE=eng

# Optional HuggingFace API Token (if using their models)
HUGGINGFACE_API_TOKEN=

# Debug mode (set to TRUE to enable debug logs)
DEBUG=FALSE